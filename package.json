{
  "name": "parcele",
  "version": "0.0.0",
  "description": "parcel + electron + react",
  "main": "app/electron.js",
  "author": "Eduardo Lopes",
  "license": "MIT",
  "scripts": {
    "parcel-server": "yarn run parcel -p 3000 app/index.html --out-dir build",
    "parcel-build": "yarn run parcel build app/index.html --out-dir build --public-url ./",
    "wait-server": "wait-on http://localhost:3000",
    "clean-build": "yarn run del build/ .cache dist/",
    "build": "yarn run clean-build && yarn run parcel-build && yarn run electron-builder --dir",
    "pack": "yarn run clean-build && yarn run parcel-build && yarn run electron-builder",
    "dev": "yarn run del build/ && yarn run concurrently \"yarn run parcel-server\" \"yarn run wait-server && cross-env NODE_ENV=development electron .\""
  },
  "build": {
    "appId": "com.parcele",
    "files": [
      "app/electron.js",
      "build/**/*"
    ]
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "speedtest-net": "^1.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "sass": "^1.25.0",
    "wait-on": "^4.0.0"
  }
}
